                </button>
                <a class="navbar-brand" href="login.html">Smartcampus TestBed</a>
            </div>
      </div>
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <div class="login-panel panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">App Sign Up Form</h3>
                    </div>
                    <div class="panel-body">
                        <form  id="app-register"  action="/app-signup" autocomplete="on"  method="post"> 
                                
				<fieldset>
                                <div class="form-group">
                                    <input class="form-control" placeholder="UserName" required="required" name="username" type="text" autofocus>
                                </div>
				<div class="form-group">
                                    <input class="form-control" placeholder="E-mail"  required="required" name="email" type="email" >
                                </div>
                                <div class="form-group">
                                    <input class="form-control" placeholder="Password"  required="required" name="password" type="password" value="">
                                </div>				
				 <div class="form-group">
                                    <input class="form-control" placeholder="Full Name" name="name" type="text" >
                                </div>
				 <div class="form-group">
                                    <input class="form-control" placeholder="Location" name="userLocation" type="text" >
                                </div>                                
                            </fieldset>
			       <button class="btn btn-primary btn-lg btn-block">Sign Up</button> 
                                <div class="panel-body">  
					Already a member ?
					<a href="/app-login" class="to_register"> Go and log in </a>
				</div>
                            </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- jQuery -->
